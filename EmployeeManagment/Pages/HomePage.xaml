<Page x:Class="EmployeeManagment.Pages.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EmployeeManagment.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HomePage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="5*"/>

        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="01" VerticalAlignment="Center">
            <TextBlock Name="Welcome" TextAlignment="Center" TextWrapping="WrapWithOverflow" FontSize="16"/>
        </StackPanel>
    </Grid>
</Page>
